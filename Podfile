# platform :ios, '9.0'

target 'Nokorimono' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Nokorimono
    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'AlamofireImage'
    pod 'Parse'

  target 'NokorimonoTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire'
  end

  target 'NokorimonoUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire'
  end

end
